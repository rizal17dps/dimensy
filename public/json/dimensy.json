    "servers": [
        {
          "url": "https://api.dimensy.id",
          "description": "The production API server",
          "variables": {
            "username": {
              "default": "demo",
              "description": "this value is assigned by the service provider, in this example `gigantic-server.com`"
            },
            "port": {
              "enum": [
                "8443",
                "443"
              ],
              "default": "443"
            },
            "basePath": {
              "default": "v1"
            }
          }
        },
        {
            "url": "https://api-sandbox.dimensy.id",
            "description": "The Sandbox API server",
            "variables": {
              "username": {
                "default": "demo",
                "description": "this value is assigned by the service provider, in this example `gigantic-server.com`"
              },
              "port": {
                "enum": [
                  "8443",
                  "443"
                ],
                "default": "443"
              },
              "basePath": {
                "default": "v1"
              }
            }
          }
      ],
    "schemes":["https","http"],
    "paths":
    {
        "/api/sendDocument": 
        {
            "post": 
            {
                "summary":"Send Document",
                "description": "Sending document",
                "consumes":["application/json"],
                "produces":["application/json"],
                "parameters":[
                    {
                        "name":"apiKey",
                        "in":"header",
                        "description":"API Key from dashboard",
                        "required":true,
                        "schema": {  
                            "type" : "string"
                        }
                    },
                    {
                        "name":"email",
                        "in":"header",
                        "description":"Email User",
                        "required":true,
                        "schema": {  
                            "type" : "string"
                        },
                        "examples" : {
                            "email1" : {
                                "value" : "example@email.com",
                                "summary" : "Sample e-mail format"
                            }
                        }
                    },
                    {
                        "name":"body",
                        "in":"body",
                        "description":"File Information",
                        "required":true,
                        "schema": { "$ref": "#/definitions/Document" },
                        "examples" : { "$ref" : "#/components/examples/bodySend" }
                    }
                ],
                "responses":{
                    "200":{
                        "description":"successful operation",
                        "schema":{
                            "$ref": "#/definitions/ApiResponse"
                        }
                    },
                    "400":{
                        "description":"unsuccessful operation",
                        "schema":{
                            "$ref": "#/definitions/ApiResponseFail"
                        }
                    }
                }
            }
        },
        "/api/getDocument": 
        {
            "get": 
            {
                "summary":"Get Documents",
                "description": "Receive documents available to be signed",
                "consumes":["application/json"],
                "produces":["application/json"],
                "parameters":[
                    {
                        "name":"apiKey",
                        "in":"header",
                        "description":"API Key from dashboard",
                        "required":true,
                        "schema": {  
                            "type" : "string"
                        }
                    },
                    {
                        "name":"email",
                        "in":"header",
                        "description":"Email User",
                        "required":true,
                        "schema": {  
                            "type" : "string"
                        },
                        "examples" : {
                            "email1" : {
                                "value" : "example@email.com",
                                "summary" : "Sample e-mail format"
                            }
                        }
                    }
                ],
                "responses":{
                    "200":{
                        "description":"successful operation",
                        "schema":{
                            "type": "object",
                            "properties": {
                                "code": { "type": "integer", "format": "int32", "example" : 200 },
                                "data" : {
                                    "type" : "array",
                                    "items" : {
                                        "type" : "object",
                                        "properties" : {
                                            "dataId": { "type": "integer", "format" :"int32" },
                                            "fileName" : {
                                                "type" : "string",
                                                "example" : "data.pdf"
                                            },
                                            "status" : {
                                                "type" : "string",
                                                "example" : "ONPROGRESS"
                                            }
                                        }
                                    }
                                },
                                "message": { "type": "string", "example" : "Success" }
                            }
                        }
                    },
                    "400":{
                        "description":"unsuccessful operation",
                        "schema":{
                            "$ref": "#/definitions/ApiResponseFail"
                        }
                    }
                }
            }
        },
