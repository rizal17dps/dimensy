    "servers": [
        {
          "url": "https://api.dimensy.id",
          "description": "The production API server",
          "variables": {
            "username": {
              "default": "demo",
              "description": "this value is assigned by the service provider, in this example `gigantic-server.com`"
            },
            "port": {
              "enum": [
                "8443",
                "443"
              ],
              "default": "443"
            },
            "basePath": {
              "default": "v1"
            }
          }
        },
        {
            "url": "https://api-sandbox.dimensy.id",
