        "/sendDocument": 
        {
            "post": 
            {
                "summary":"Send Document",
                "description": "Sending document",
                "consumes":["application/json"],
                "produces":["application/json"],
                "parameters":[
                    {
                        "name":"apiKey",
                        "in":"header",
                        "description":"API Key from dashboard",
                        "required":true,
                        "schema": {  
                            "type" : "string"
                        }
                    },
                    {
                        "name":"email",
                        "in":"header",
                        "description":"Email User",
                        "required":true,
                        "schema": {  
                            "type" : "string"
                        },
                        "examples" : {
                            "email1" : {
                                "value" : "example@email.com",
                                "summary" : "Sample e-mail format"
                            }
                        }
                    },
                    {
                        "name":"body",
                        "in":"body",
                        "description":"File Information",
                        "required":true,
                        "schema": { "$ref": "#/definitions/Document" },
                        "examples" : { "$ref" : "#/components/examples/bodySend" }
                    }
                ],
                "responses":{
                    "200":{
                        "description":"successful operation",
                        "schema":{
                            "$ref": "#/definitions/ApiResponse"
                        }
                    },
                    "400":{
                        "description":"unsuccessful operation",
                        "schema":{
                            "$ref": "#/definitions/ApiResponseFail"
                        }
                    }
                }
            }
        },
